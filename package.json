    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "inquirer": "^8.2.4",
    "jsdom": "^19.0.0",
    "pnpm": "^7.5.1",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3",
    "vite": "^3.0.0",
